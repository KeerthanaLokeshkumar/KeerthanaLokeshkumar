package project40;

public class TryCatch {
	    public static void main(String args[]) 
	    {
	        int[] array = new int[5];
	        try 
	        {
	            array[8] = 2;
	        }
	        catch (ArrayIndexOutOfBoundsException e) 
	        {
	            System.out.println("Array index"); 
	        }
	        finally 
	        {
	            System.out.println("The arraysize " + array.length);
	        }
	    }
	}
