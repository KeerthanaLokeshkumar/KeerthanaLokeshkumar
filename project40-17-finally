package project40;

public class Finally {
        public static void main(String[] args)
        {
            int n1=45,n2=0,r=0;
            try
            {
                r = n1/ n2;
            }
            catch(ArithmeticException Ex)
            {
                System.out.print("\n\tError : " + Ex.getMessage());
            }
            finally
            {
                System.out.print("\n\tThe result is : " + r);
            }
        }
    }



