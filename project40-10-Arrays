package project40;

public class Arrays {

		public static void main(String[] args) {
		int a1[]= {10,20,30,40,50};
		for(int i=0;i<5;i++) {
		System.out.println("Elements of array a: "+a1[i]);
		}

		
		int[][] b1 = {
		            {2, 4, 6, 8}, 
		            {3, 6, 9} };
		      
		      System.out.println("\nLength of row 1: " + b1[0].length);
		      }


}
